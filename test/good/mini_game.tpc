struct pair game(struct pair numbers) {
    while (numbers.guessed != numbers.to_find) {
        numbers.attempts = numbers.attempts + 1;
        print_attempts(numbers.attempts);
        reade(numbers.guessed);
        if (numbers.guessed > numbers.to_find) {
            hint_less();
        }
        if (numbers.guessed < numbers.to_find) {
            hint_more();
        }
    }
    return numbers;
}

